{
  "name": "lukeboyle",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=\"production\" npm run build-core",
    "build-core": "concurrently \"npm run build-js\" \"npm run build-styles\"",
    "build-js": "webpack -p --config webpack.config.js --color --display-error-details",
    "build-styles": "node-sass ./src/sass --output ./assets",
    "watch": "npm run build && npm run watch-js & npm run watch-styles",
    "watch-js": "webpack --config webpack.config.js --progress --color --display-error-details --watch --debug --output-pathinfo",
    "watch-styles": "node-sass ./src/sass --output ./assets --watch"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-istanbul": "^2.0.0",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015-native-modules": "^6.9.2",
    "babel-preset-es2017": "^6.14.0",
    "concurrently": "^2.2.0",
    "cpx": "^1.3.2",
    "node-sass": "^3.7.0",
    "webpack": "^2.1.0-beta.21"
  },
  "dependencies": {
    "@lukeboyle/mouse-near": "^1.0.0"
  }
}
