{
  "name": "lukeboyle",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "yarn && concurrently \"npm run watch\" \"webpack-dev-server\"",
    "clean": "rm -rf dist",
    "prepare-dirs": "mkdir dist",
    "build": "NODE_ENV=production && npm run build-primer && concurrently \"npm run build-core\"",
    "build-primer": "npm run clean && npm run prepare-dirs",
    "build-core": "concurrently \"npm run build-js\" \"npm run build-html\"",
    "build-js": "webpack --config webpack.config.js --color --display-error-details",
    "build-html": "cp -a ./src/assets/html/ ./dist",
    "watch": "NODE_ENV=development && npm run build && npm run watch-js & npm run watch-html",
    "watch-js": "webpack --config webpack.config.js --progress --color --devtool=sourcemap --display-error-details --watch --debug --output-pathinfo",
    "watch-html": "cpx \"src/assets/html/**/*\" dist --watch -v",
    "generate-docs": "jsdoc src/js/ -r -c jsdoc.conf.json"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-istanbul": "^2.0.0",
    "babel-plugin-transform-async-functions": "^6.22.0",
    "babel-plugin-transform-es2015-destructuring": "^6.16.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.16.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-native-modules": "^6.9.2",
    "babel-preset-es2017": "^6.9.2",
    "babel-preset-react": "^6.11.1",
    "browser-sync": "^2.18.12",
    "concurrently": "^2.2.0",
    "cpx": "^1.3.2",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "fs-extra": "^3.0.1",
    "jsdoc": "^3.4.1",
    "markdown-loader": "^2.0.0",
    "node-sass": "^4.5.3",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-icons": "^2.2.5",
    "react-router": "^4.1.1",
    "react-static-webpack-plugin": "^2.1.0",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {}
}
