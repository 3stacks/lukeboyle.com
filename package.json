{
  "name": "luke-boyle-portfolio",
  "version": "2.0.0",
  "description": "My personal website",
  "scripts": {
    "generate-artist-data": "ts-node scripts/music.ts",
    "generate-portfolio-items": "ts-node scripts/portfolio.ts",
    "generate-blog-posts": "node scripts/blog-post.js",
    "start": "npm run generate-artist-data && npm run generate-blog-posts && npm run generate-portfolio-items && npm run develop",
    "develop": "gatsby develop",
    "build": "NODE_ENV=production npm run generate-artist-data && npm run generate-blog-posts && npm run generate-portfolio-items && gatsby build",
    "deploy": "node scripts/deploy.js",
    "server:build": "tsc scripts/server.ts",
    "server:start": "nodemon 'scripts/server.ts' --exec 'ts-node' scripts/server.ts -e ts,graphql"
  },
  "author": "Luke Boyle",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@types/lodash": "^4.14.138",
    "@types/shelljs": "^0.8.5",
    "apollo-server": "^2.8.1",
    "date-fns": "^1.29.0",
    "gatsby-link": "latest",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-helmet": "^5.2.0",
    "react-icons": "^3.2.2",
    "react-typography": "^0.15.10",
    "styled-components": "^4.0.3",
    "typography": "^0.15.10"
  },
  "devDependencies": {
    "@lukeboyle/get-filename-from-path": "^1.0.3",
    "@types/fs-extra": "^8.0.0",
    "@types/marked": "^0.6.5",
    "@types/node": "^12.7.3",
    "aws-sdk": "^2.329.0",
    "axios": "^0.18.0",
    "babel-plugin-styled-components": "^1.8.0",
    "camel-case": "^3.0.0",
    "dotenv": "^6.0.0",
    "fs-extra": "^6.0.1",
    "gatsby": "2.13.82",
    "gatsby-plugin-manifest": "^2.0.5",
    "gatsby-plugin-offline": "^2.0.11",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-sharp": "^2.0.7",
    "gatsby-plugin-sitemap": "^2.2.9",
    "gatsby-plugin-styled-components": "^3.0.1",
    "gatsby-source-filesystem": "^2.0.4",
    "gatsby-transformer-sharp": "^2.1.4",
    "glob": "^7.1.3",
    "lodash": "^4.17.11",
    "marked": "^0.7.0",
    "mime-types": "^2.1.20",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "prop-types": "^15.6.0",
    "shelljs": "^0.8.2",
    "title-case": "^2.1.1",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  }
}
